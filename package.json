{
  "name": "vite-plugin-monaco",
  "type": "module",
  "version": "0.0.0",
  "description": "A Vite plugin for the Monaco Editor.",
  "author": "Yue JIN <yuejin13@fudan.edu.cn>",
  "packageManager": "pnpm@10.17.0",
  "license": "MIT",
  "keywords": [
    "monaco",
    "editor",
    "vite",
    "vite-plugin"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "client.d.ts"
  ],
  "exports": {
    "./client": {
      "types": "./client.d.ts"
    },
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./*": "./*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kingyue737/vite-plugin-monaco.git"
  },
  "homepage": "https://github.com/kingyue737/vite-plugin-monaco#readme",
  "bugs": {
    "url": "https://github.com/kingyue737/vite-plugin-monaco/issues"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm",
    "release": "bumpp",
    "changelog": ""
  },
  "devDependencies": {
    "bumpp": "^10.2.3",
    "monaco-editor": "^0.53.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.6"
  },
  "peerDependencies": {
    "monaco-editor": ">= 0.31.0",
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0"
  }
}
